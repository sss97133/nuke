{
  "editable_files": [
    "src/components/**/*.tsx",
    "src/components/**/*.ts",
    "src/pages/**/*.tsx",
    "src/pages/**/*.ts",
    "src/hooks/**/*.ts",
    "src/utils/**/*.ts",
    "src/types/**/*.ts",
    "src/**/*.css",
    "docs/**/*.md"
  ],
  "read_only_files": [
    "vite.config.ts",
    "package.json",
    "tsconfig.json",
    "tailwind.config.ts",
    ".env"
  ],
  "permissions": {
    "allow_file_creation": true,
    "allow_file_deletion": false,
    "allow_file_editing": true
  }
}
