# Nuke Project Environment Variables

# API Endpoints
VITE_API_BASE_URL=http://localhost:3000/api

# Authentication
VITE_AUTH_DOMAIN=yourdomain.auth.region.supabase.co
VITE_AUTH_CLIENT_ID=your-client-id
VITE_AUTH_AUDIENCE=your-audience
VITE_AUTH_REDIRECT_URI=http://localhost:8080/callback

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_EXPERIMENTAL_FEATURES=false

# Mapbox (if used)
VITE_MAPBOX_ACCESS_TOKEN=your-mapbox-access-token

# Supabase (if used)
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Sentry Configuration
VITE_SENTRY_DSN=your_sentry_dsn

# SpatialLM Configuration
SPATIALLM_MODEL_PATH=/models/spatiallm/model.pt
SPATIALLM_DEVICE=cuda
SPATIALLM_BATCH_SIZE=32
SPATIALLM_CONFIDENCE_THRESHOLD=0.5
SPATIALLM_MAX_OBJECTS=100
SPATIALLM_MIN_OBJECT_SIZE=0.1
SPATIALLM_MAX_OBJECT_SIZE=10.0
SPATIALLM_POINT_CLOUD_DENSITY=0.1
SPATIALLM_PROCESSING_TIMEOUT=30000
SPATIALLM_PYTHON_PATH=python

# Other Configuration
VITE_API_URL=http://localhost:3000
VITE_WS_URL=ws://localhost:3000

# Application Settings
VITE_APP_NAME="Nuke Project"
VITE_APP_DESCRIPTION="Modern React application with TypeScript and shadcn/ui components"
VITE_APP_VERSION=0.1.0
VITE_DEFAULT_THEME=system

# This is just an example file. Create a .env.local file with your actual values.
# Never commit actual .env files to your repository.
