{
  "buildCommand": "cd nuke_frontend && npm run build",
  "outputDirectory": "nuke_frontend/dist",
  "installCommand": "cd nuke_frontend && npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://nuke-api.fly.dev/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}