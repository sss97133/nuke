# Vercel AI Gateway Configuration
# Add these to your .env or Vercel environment variables

# Gateway ID (from Vercel Dashboard > AI Gateway)
VITE_VERCEL_AI_GATEWAY_ID=nzero_key

# Gateway API Key (from Vercel Dashboard > AI Gateway)
VITE_VERCEL_AI_GATEWAY_KEY=<your-vercel-ai-gateway-key>

# Enable AI Gateway
VITE_VERCEL_AI_GATEWAY_ENABLED=true