{
  "buildCommand": "CI=false npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "functions": {
    "api/quickbooks/callback.ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/$",
      "headers": {
        "Cache-Control": "no-store"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block"
      },
      "continue": true
    },
    {
      "src": "/api/quickbooks/callback",
      "dest": "/api/quickbooks/callback.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "https://qkgaybvrernstplzjaam.functions.supabase.co/mailbox/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}