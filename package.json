    "axios": "^1.8.4",
    "chalk": "^5.4.1",
    "cheerio": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dompurify": "^3.2.4",
    "dotenv": "^16.4.7",
    "embla-carousel-react": "^8.5.2",
    "ethers": "^6.13.5",
    "framer-motion": "^12.4.11",
    "input-otp": "^1.4.2",
    "jotai": "^2.12.2",
    "lovable-tagger": "^1.1.7",
    "lucide-react": "^0.483.0",
    "mapbox-gl": "^3.10.0",
    "marked": "^15.0.7",
    "next-themes": "^0.4.5",
    "node-fetch": "^3.3.2",
    "papaparse": "^5.5.2",
    "react": "^19.0.0",
    "react-day-picker": "^9.6.3",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.2",
    "react-qr-code": "^2.0.15",
    "react-resizable-panels": "^2.1.7",
    "react-router-dom": "^7.4.0",
    "recharts": "^2.15.1",
    "shiki": "^3.2.1",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.15",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.174.0",
    "uuid": "^11.1.0",
    "validator": "^13.12.0",
