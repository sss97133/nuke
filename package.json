{
  "name": "nuke",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production tsc && vite build && node scripts/inject-env.js",
    "build:alt": "cross-env NODE_ENV=production node build.js",
    "build:esm": "cross-env NODE_ENV=production node build.mjs",
    "build:ci": "cross-env CI=true NODE_ENV=production node build.mjs",
    "build:dev": "cross-env NODE_ENV=development npm run validate:all && vite build --mode development",
    "build:prod": "cross-env NODE_ENV=production tsc && vite build --mode production && node scripts/copy-gm-records.js && node scripts/inject-env.js && node scripts/fix-production-assets.js && node scripts/verify-env.js",
    "vercel-build": "npm ci && npm run build:prod",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:queries": "node scripts/validate-supabase-queries.mjs",
    "lint:queries:fix": "node scripts/validate-supabase-queries.mjs --fix",
    "validate:schema": "cross-env NODE_ENV=test NODE_OPTIONS='--loader ts-node/esm --no-warnings' node scripts/validate-schema.ts",
    "validate:all": "npm run type-check && npm run lint && npm run lint:queries && npm run validate:schema && npm run verify:docs",
    "fix-deps": "node scripts/fix-deps.js",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "test:timeline": "node --experimental-json-modules scripts/test-vehicle-timeline.js",
    "setup:timeline": "node scripts/fix-production-assets.js && node scripts/verify-env.js && node --experimental-json-modules scripts/test-vehicle-timeline.js",
    "timeline:test": "node scripts/timeline-test-helper.js",
    "timeline:supabase": "node scripts/supabase-timeline-migration.js",
    "timeline:direct": "node scripts/direct-timeline-migration.js",
    "timeline:api": "node scripts/api-timeline-migration.js",
    "timeline:setup": "node scripts/setup-timeline-table.js",
    "timeline:check": "node scripts/check-timeline-table.js",
    "timeline:seed": "node scripts/seed-timeline-events.js",
    "timeline:import": "node scripts/real-vehicle-importer.js",
    "bat:extract": "node scripts/extract-bat-data.js",
    "timeline:seed-file": "node scripts/seed-timeline-from-file.js",
    "vehicles:make-public": "node scripts/make-test-vehicles-public.js",
    "setup:test-env": "node scripts/setup-test-env.js",
    "clean": "rimraf dist node_modules/.vite",
    "prepare": "husky",
    "test": "NODE_OPTIONS='--max-old-space-size=8192' vitest",
    "test:ci": "vitest run --coverage",
    "test:watch": "NODE_OPTIONS='--max-old-space-size=8192' vitest watch",
    "postbuild": "echo 'Build completed successfully!'",
    "verify:docs": "node scripts/verify-docs.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@huggingface/transformers": "^3.4.0",
    "@radix-ui/react-accordion": "^1.2.3",
